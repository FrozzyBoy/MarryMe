<!DOCTYPE html>
<html ng-app="marryApp">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title – Могилевский ЗАГС</title>
	@Styles.Render("~/Content/css")
	<link href="~/Content/Tile.css" rel="stylesheet" />
	<link href="~/Content/responsive-calendar.css" rel="stylesheet" />
	<link href="~/Content/animation.css" rel="stylesheet" />
	<link href="~/Content/Month.css" rel="stylesheet" />
	<link href="~/Content/Hall.css" rel="stylesheet" />
	<link href="~/Content/Time.css" rel="stylesheet" />
	<link href="~/Content/map.css" rel="stylesheet" />
	<link href="~/Content/Loader.css" rel="stylesheet" />
	@*<link href="~/Content/font-awesome.min.css" rel="stylesheet" />*@
	@*<link href="~/Content/velvet.css" rel="stylesheet" />*@
	<link href="~/Content/custom.css" rel="stylesheet" />
	@*@Scripts.Render("~/bundles/modernizr")*@

</head>
<body>
	<div id="load" style="background: url('../img/loading.gif') center no-repeat #fff; width:100%; height:100%; position:fixed; z-index:9000"></div>
	<div id="site" ng-hide="hide" ng-cloak>
		<nav class="navbar navbar-default navbar-fixed-top">
			<div class="container">
				<div class="navbar-header">
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand smooth-scroll" href="#intro"><span class="navbar-top-text"> Главная страница</span></a>
				</div>
				<div class="navbar-collapse collapse">
					<ul class="nav navbar-nav navbar-right">
						<li><a href="#mission" class="smooth-scroll navbar-top-text">Выбор даты</a></li>
						<li><a href="#services" class="smooth-scroll navbar-top-text">Выбор зала</a></li>
						<li><a href="#contact" class="smooth-scroll navbar-top-text">Контакты</a></li>
					</ul>
				</div>
			</div>
		</nav>

		@RenderBody()
	</div>

	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	<script src="http://maps.googleapis.com/maps/api/js?v=3.14&sensor=false"></script>
	<script src="~/Scripts/angular.min.js"></script>
	<script src="~/Scripts/angular-ui/ui-bootstrap.js"></script>
	<script src="~/Scripts/angular-ui/ui-bootstrap-tpls.js"></script>
	<script src="~/Scripts/lodash.js"></script>
	<script src="~/Scripts/angular-google-maps.js"></script>
	<script src="~/Scripts/Logic/marryMeApp.js"></script>

	<script src="~/Scripts/responsive-calendar.js"></script>
	<script src="~/Scripts/Logic/Tile.js"></script>
	<script src="~/Scripts/Logic/SmoothScroll.js"></script>

	<script src="~/Scripts/Style/pollyfill.js"></script>
	<script src="~/Scripts/Style/parallax.min.js"></script>
	<script src="~/Scripts/Detect/detect.min.js"></script>
	<script src="~/Scripts/Style/IEScript.js"></script>
	<script src="https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit" async defer></script>
	@*<script src="~/Scripts/Style/curtains.js"></script>*@
	@*<script src="~/Scripts/Style/velvet.js"></script>*@
	<script src="~/Scripts/Style/smoothscroll.js"></script>
	<script src="~/Scripts/jquery.maskedinput.min.js"></script>
	<script src="~/Scripts/Logic/telephoneMask.js"></script>
	<script src="~/Scripts/Logic/CopyRights.js"></script>
	@RenderSection("scripts", required: false)
</body>
</html>
